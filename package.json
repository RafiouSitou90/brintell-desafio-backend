{
  "name": "brintell-desafio-backend",
  "version": "1.0.0",
  "description": "Brintell desafio backend",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node --transpile-only src/server.ts",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm:cli": "ts-node-dev node_modules/typeorm/cli -d ./src/data-source.ts",
    "typeorm-generate": "npm run typeorm:cli migration:generate ./src/migrations -n"
  },
  "keywords": [
    "Brintell",
    "Desafio"
  ],
  "author": {
    "name": "Rafiou Sitou",
    "email": "rafiousitou90@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.10",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": "14.x"
  }
}
